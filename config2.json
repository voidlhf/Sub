{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "address": "https://1.1.1.1/dns-query",
        "address_resolver": "dns_resolver",
        "strategy": "ipv4_only",
        "detour": "select"
      },
      {
        "tag": "dns_direct",
        "address": "https://223.5.5.5/dns-query",
        "address_resolver": "dns_resolver",
        "strategy": "ipv4_only",
        "detour": "direct"
      },
      {
        "tag": "dns_resolver",
        "address": "223.5.5.5",
        "strategy": "ipv4_only",
        "detour": "direct"
      },
      {
        "tag": "dns_refused",
        "address": "rcode://refused"
      },
      {
        "tag": "dns_fakeip",
        "address": "fakeip"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "dns_resolver"
      },
      {
        "rule_set": [
          "domainset/reject",
          "domainset/reject_extra",
          "non_ip/reject"
        ],
        "server": "dns_refused",
        "disable_cache": true
      },
      {
        "rule_set": [
          "domainset/cdn",
          "geosite-geolocation-!cn",
          "non_ip/cdn",
          "non_ip/ai"
        ],
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dns_fakeip"
      },
      {
        "rule_set": [
          "domainset/cdn",
          "geosite-geolocation-!cn",
          "non_ip/cdn",
          "non_ip/ai"
        ],
        "query_type": [
          "CNAME"
        ],
        "server": "dns_proxy"
      },
      {
        "query_type": [
          "A",
          "AAAA",
          "CNAME"
        ],
        "invert": true,
        "server": "dns_refused",
        "action": "route-options",
        "disable_cache": true
      }
    ],
    "final": "dns_direct",
    "independent_cache": true,
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    }
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "address": [
        "172.18.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "stack": "system"
    }
  ],
  "outbounds": [
    {
      "tag": "chatgpt",
      "type": "selector",
      "outbounds": [
        "exclude: 香港"
      ]
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rule_set": [
      {
        "tag": "geosite-geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-!cn.srs"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs"
      },
      {
        "tag": "domainset/reject",
        "type": "remote",
        "format": "source",
        "url": "https://ruleset.skk.moe/sing-box/domainset/reject.json"
      },
      {
        "tag": "domainset/reject_extra",
        "type": "remote",
        "format": "source",
        "url": "https://ruleset.skk.moe/sing-box/domainset/reject_extra.json"
      },
      {
        "tag": "non_ip/reject",
        "type": "remote",
        "format": "source",
        "url": "https://ruleset.skk.moe/sing-box/non_ip/reject.json"
      },
      {
        "tag": "non_ip/ai",
        "type": "remote",
        "format": "source",
        "url": "https://ruleset.skk.moe/sing-box/non_ip/ai.json"
      },
      {
        "tag": "domainset/cdn",
        "type": "remote",
        "format": "source",
        "url": "https://ruleset.skk.moe/sing-box/domainset/cdn.json"
      },
      {
        "tag": "non_ip/cdn",
        "type": "remote",
        "format": "source",
        "url": "https://ruleset.skk.moe/sing-box/non_ip/cdn.json"
      }
    ],
    "rules": [
      {
        "inbound": "tun-in",
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "port": 853,
        "network": "tcp",
        "action": "reject"
      },
      {
        "port": 443,
        "network": "udp",
        "action": "reject"
      },
      {
        "rule_set": [
          "domainset/reject",
          "domainset/reject_extra",
          "non_ip/reject"
        ],
        "action": "reject"
      },
      {
        "package_name": [
          "com.openai.chatgpt"
        ],
        "outbound": "chatgpt"
      },
      {
        "rule_set": [
          "non_ip/ai"
        ],
        "outbound": "chatgpt"
      },
      {
        "port": [
          23301,
          22101,
          22102,
          20501
        ],
        "outbound": "direct"
      },
      {
        "rule_set": [
          "domainset/cdn",
          "geosite-geolocation-!cn",
          "non_ip/cdn"
        ],
        "outbound": "select"
      },
      {
        "rule_set": "geoip-cn",
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      }
    ],
    "final": "select",
    "auto_detect_interface": true,
    "override_android_vpn": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "0.0.0.0:9090"
    }
  }
}