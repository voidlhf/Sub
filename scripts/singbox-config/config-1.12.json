{
  "log": {},
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "type": "https",
        "server": "1.1.1.1",
        "detour": "select"
      },
      {
        "tag": "dns_direct",
        "type": "https",
        "server": "223.5.5.5"
      },
      {
        "tag": "dns_fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "query_type": "HTTPS"
          },
          {
            "rule_set": [
              "geosite-google-ads@ads",
              "geosite-spotify-ads@ads",
              "AdGuardSDNSFilter"
            ]
          }
        ],
        "action": "reject"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dns_fakeip",
        "rewrite_ttl": 1
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "clash_mode": "Global"
          }
        ],
        "server": "dns_proxy"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "clash_mode": "Direct"
          },
          {
            "rule_set": [
              "geosite-cn"
            ]
          }
        ],
        "server": "dns_direct"
      }
    ],
    "strategy": "ipv4_only",
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "tun",
      "address": [
        "172.18.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "auto_route": true,
      "strict_route": true,
      "route_exclude_address": [
        "192.168.0.0/16",
        "fc00::/7"
      ]
    }
  ],
  "outbounds": [],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "action": "resolve",
        "strategy": "ipv4_only"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "domain_suffix": [
              "ushd.xmsl.org",
              "cf.gtlx.org",
              "public-operation-hkrpg-sg.hoyoverse.com",
              "public-operation-hk4e-sg.hoyoverse.com",
              "public-operation-nap-sg.hoyoverse.com"
            ]
          },
          {
            "port_range": [
              "23301:23301",
              "22101:22102",
              "20501:20501",
              "21601:21606"
            ]
          }
        ],
        "outbound": "direct"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "clash_mode": "Global"
          },
          {
            "rule_set": [
              "geosite-hoyoverse"
            ]
          }
        ],
        "outbound": "select"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "rule_set": [
              "geosite-openai",
              "geosite-google-gemini"
            ]
          }
        ],
        "outbound": "ai"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "clash_mode": "Direct"
          },
          {
            "ip_is_private": true
          },
          {
            "rule_set": [
              "geosite-googlefcm",
              "geosite-cn",
              "geoip-cn"
            ]
          }
        ],
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geoip-cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geoip/geoip-cn.srs",
        "download_detour": "select"
      },
      {
        "type": "remote",
        "tag": "geosite-cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-cn.srs",
        "download_detour": "select"
      },
      {
        "type": "remote",
        "tag": "geosite-openai",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-openai.srs",
        "download_detour": "select"
      },
      {
        "type": "remote",
        "tag": "geosite-google-gemini",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-google-gemini.srs",
        "download_detour": "select"
      },
      {
        "type": "remote",
        "tag": "geosite-googlefcm",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-googlefcm.srs",
        "download_detour": "select"
      },
      {
        "type": "remote",
        "tag": "geosite-hoyoverse",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-hoyoverse.srs",
        "download_detour": "select"
      },
      {
        "type": "remote",
        "tag": "geosite-spotify-ads@ads",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-spotify-ads@ads.srs",
        "download_detour": "select"
      },
      {
        "type": "remote",
        "tag": "geosite-google-ads@ads",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-google-ads@ads.srs",
        "download_detour": "select"
      },
      {
        "type": "remote",
        "tag": "AdGuardSDNSFilter",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/xmdhs/sing-box-ruleset/rule-set/AdGuardSDNSFilterSingBox.srs",
        "download_detour": "select"
      }
    ],
    "final": "select",
    "default_domain_resolver": "dns_direct",
    "auto_detect_interface": true,
    "override_android_vpn": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090"
    }
  }
}